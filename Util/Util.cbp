				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wnon-virtual-dtor" />
			<Add option="-Winit-self" />
			<Add option="-Wredundant-decls" />
			<Add option="-Winline" />
			<Add option="-Weffc++" />
